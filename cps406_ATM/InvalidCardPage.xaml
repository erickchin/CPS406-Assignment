<Page x:Class="cps406_ATM.InvalidCardPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:cps406_ATM"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="InvalidCardPage">

    <Grid>
        <Button x:Name="button_1" Content="1" HorizontalAlignment="Left" Margin="276,377,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_2" Content="2" HorizontalAlignment="Left" Margin="316,377,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_3" Content="3" HorizontalAlignment="Left" Margin="356,377,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_4" Content="4" HorizontalAlignment="Left" Margin="276,417,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_5" Content="5" HorizontalAlignment="Left" Margin="316,417,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_6" Content="6" HorizontalAlignment="Left" Margin="356,417,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Button x:Name="button_7" Content="7" HorizontalAlignment="Left" Margin="276,457,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
