        <Button x:Name="button_cancel" Content="CANCEL" HorizontalAlignment="Left" Margin="396,417,0,0" VerticalAlignment="Top" Width="70" Height="35" Click="button_cancel_Click"/>
        <Button x:Name="button_enter" Content="ENTER" HorizontalAlignment="Left" Margin="396,457,0,0" VerticalAlignment="Top" Width="70" Height="35"/>
        <Button x:Name="button_0" Content="0" HorizontalAlignment="Left" Margin="316,497,0,0" VerticalAlignment="Top" Width="35" Height="35"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="350" Margin="100,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="600"/>
        <Label x:Name="labelStatus" Content="Invalid card, please press cancel." HorizontalAlignment="Left" Margin="200,60,0,0" VerticalAlignment="Top" FontSize="29.333"/>
        <Label x:Name="labelPin" Content="" HorizontalAlignment="Left" Margin="370,110,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.006,-0.45" FontSize="26.667"/>
        <Button x:Name="button_Copy1" Content="&lt;      " HorizontalAlignment="Left" Margin="705,200,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy2" Content="&lt;      " HorizontalAlignment="Left" Margin="705,250,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy4" Content="&lt;      " HorizontalAlignment="Left" Margin="705,150,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy5" Content="&lt;      " HorizontalAlignment="Left" Margin="705,300,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy6" Content="      &gt;" HorizontalAlignment="Left" Margin="35,200,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy7" Content="      &gt;" HorizontalAlignment="Left" Margin="35,250,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy8" Content="      &gt;" HorizontalAlignment="Left" Margin="35,150,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
        <Button x:Name="button_Copy9" Content="      &gt;" HorizontalAlignment="Left" Margin="35,300,0,0" VerticalAlignment="Top" Width="60" Height="30" FontSize="18.667"/>
